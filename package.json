{
  "name": "world-clocks-new-tab",
  "version": "1.1.2",
  "description": "WorldClocks New Tab - Chrome extension to visualise different time zones on each tab",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build index.html --public-url ./ && npm run manifest && npm run assets && npm run compress",
    "compress": "zip -r world-clocks-new-tab.zip dist/",
    "manifest": "cp manifest.json dist/",
    "assets": "cp -R assets dist/",
    "start": "parcel index.html"
  },
  "keywords": [
    "new-tab",
    "chrome-extension",
    "clocks",
    "time",
    "world-clocks",
    "timezones"
  ],
  "author": "Alexander Odell <alextrastero@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "localStorage": "^1.0.3",
    "nervjs": "^1.2.8",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.4.1"
  }
}
